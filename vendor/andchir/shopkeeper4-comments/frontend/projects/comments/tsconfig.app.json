{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "preserveSymlinks": true,
    "outDir": "../../out-tsc/app",
    "types": [],
    "baseUrl": "../../src",
    "paths": {
      "@app/*": [
        "app/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
